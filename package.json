{
    "name": "livereloaddebugger",
    "displayName": "Livereload Debugger",
    "description": "Livereload Debugger",
    "version": "0.0.1",
    "publisher": "Bruno De Nadai Sarnaglia",
    "engines": {
        "vscode": "^1.21.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.debug.watch",
                "title": "Livereload Debugger"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test",
        "install-local": "rm -rf ~/.vscode/extensions/debug-watch-cs-extension/ && cp -r ./ ~/.vscode/extensions/debug-watch-cs-extension"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "@types/sleep": "0.0.7",
        "sleep": "^5.1.1"
    }
}
